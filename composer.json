{
    "name": "laravapi/twitter",
    "description": "This is my package twitter",
    "keywords": [
        "laravapi",
        "laravel",
        "twitter"
    ],
    "homepage": "https://github.com/laravapi/twitter",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Sch√∂ps",
            "email": "sebastian.schoeps@nanuc.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "abraham/twitteroauth": "^3.2",
        "illuminate/contracts": "^8.73",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Laravapi\\Twitter\\": "src",
            "Laravapi\\Twitter\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravapi\\Twitter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravapi\\Twitter\\TwitterServiceProvider"
            ],
            "aliases": {
                "Twitter": "Laravapi\\Twitter\\Facades\\Twitter"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
